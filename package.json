{
  "name": "ai-fashion-collective",
  "version": "1.0.0",
  "description": "AI-powered collective fashion design platform",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "ai": "cd ai && python main.py",
    "all": "concurrently \"npm run server\" \"npm run client\" \"npm run ai\"",
    "install-all": "npm install && cd frontend && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJshome/ai-fashion-collective.git"
  },
  "keywords": [
    "ai",
    "fashion",
    "design",
    "collective",
    "platform"
  ],
  "author": "Jihwan Jang",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/JJshome/ai-fashion-collective/issues"
  },
  "homepage": "https://github.com/JJshome/ai-fashion-collective#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
